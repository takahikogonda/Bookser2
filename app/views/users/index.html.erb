<%= render 'users/side', book:@book %>
	<div class="col-xs-9">
	<h1>Users</h1>
		<table class="table">
			<thead>
				<tr>
					<th>image</th>
					<th>name</th>
				</tr>
			</thead>
			<tbody>
				<tr>
					<% @users.each do |user| %>
					<tr>
						<td><%= attachment_image_tag user, :profile_image, size: "60x60" ,fallback: "no_image" %></td>
						<td><%= user.name %></td>
						<td><%= link_to "Show",user_path(user)%></td>
					</tr>
				<% end %>
				</tr>
			</tbody>
		</table>
	</div>